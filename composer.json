{
    "name": "interfrog/if_basic",
    "type": "typo3-cms-extension",
    "version": "3.0.0",
    "description": "IfPage Basic - Distribution package based upon the if_themeconfiguration extension with basic template and styles",
    "keywords": [
        "TYPO3",
        "extension",
        "ifPage",
        "Theme",
        "Basic"
    ],
    "homepage": "https://www.ifpage.de",
    "authors": [{
        "name": "Interfrog Produktion GmbH",
        "role": "Developer",
        "homepage": "http://interfrog.de"
    }],
    "license": [
        "GPL-2.0+"
    ],
    "require": {
        "typo3/cms-core": "9.5.*",
        "interfrog/if_themeconfiguration": "3.*"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "autoload": {
        "psr-4": {
            "Interfrog\\IfBasic\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web"
        }
    }
}